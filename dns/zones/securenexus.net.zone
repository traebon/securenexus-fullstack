
securenexus.net. IN SOA ns1.securenexus.net. admin.securenexus.net. (
    2025110501 ; serial (updated: added Dickson ERP records)
    3600       ; refresh
    1800       ; retry
    604800     ; expire
    86400      ; minimum
)
securenexus.net. IN NS ns1.securenexus.net.
securenexus.net. IN NS ns2.securenexus.net.
securenexus.net. IN A 137.74.40.208
ns1.securenexus.net. IN A 137.74.40.208
ns2.securenexus.net. IN A 137.74.40.208
*.securenexus.net. IN A 137.74.40.208

; Mail server records
mail.securenexus.net. IN A 137.74.40.208
securenexus.net. IN MX 10 mail.securenexus.net.

; SPF record
securenexus.net. IN TXT "v=spf1 mx ~all"

; DMARC policy
_dmarc.securenexus.net. IN TXT "v=DMARC1; p=quarantine; rua=mailto:admin@securenexus.net"

; DKIM key
dkim._domainkey.securenexus.net. IN TXT "v=DKIM1;k=rsa;t=s;s=email;"

; TLSA record for SMTP (port 25)
_25._tcp.mail.securenexus.net. IN TLSA 3 1 1 2fabf5f7386428c24806fe52d2ccb44d2546396ee2e488983120d488ef193352

; Autodiscover/Autoconfig (Exchange/Outlook/Thunderbird)
autodiscover.securenexus.net. IN CNAME mail.securenexus.net.
autoconfig.securenexus.net. IN CNAME mail.securenexus.net.
_autodiscover._tcp.securenexus.net. IN SRV 0 1 443 mail.securenexus.net.

; Dickson Supplies ERP (Client tenant)
erp.dickson.securenexus.net. IN A 137.74.40.208
pos.dickson.securenexus.net. IN A 137.74.40.208

